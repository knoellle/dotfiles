super + Return
    $TERMINAL
XF86Audio{Prev,Next,Play}
    mpc -q {prev,next,toggle}
Print
    screenshot
XF86AudioMute
    amixer -q set Master toggle
XF86Audio{Raise,Lower}Volume
    amixer -q set Master 1%{+,-}
super + c
    $BROWSER
XF86Launch1
    locknow && xset dpms force off
{XF86PowerOff, XF86Sleep}
    locknow && systemctl suspend
{XF86ScreenSaver, super + control + l}
    locknow
XF86RotateWindows
    flock -n /tmp/rotscreen.lock -c 'rotate-screen;sleep 1'
super + bracketleft
    zsh -i -c mate
super + bracketright
    termite -e "zsh -i -c simrobert"
super + e
    dmenuunicode
super + shift + Return
    spawn

